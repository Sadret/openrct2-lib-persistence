{
    "compilerOptions": {
        "target": "ES5",
        "module": "ESNext",
        "lib": ["ESNext"],
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@src/*": ["./src/*"]
        },
    },
    "include": [
        "./src/**/*.ts",
    ]
}